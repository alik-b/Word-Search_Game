<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.mediaview.MediaViewController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="451.0" />
      <Button fx:id="playButton" layoutX="142.0" layoutY="327.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="pauseButton" layoutX="241.0" layoutY="327.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="resetButton" layoutX="341.0" layoutY="327.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>
