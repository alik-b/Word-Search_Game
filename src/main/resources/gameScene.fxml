<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GameController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/gameBackground.jpg" />
         </image>
      </ImageView>
      <Button fx:id="homeButton" layoutY="1.0" mnemonicParsing="false" onAction="#goHome" />
      <ScrollPane fx:id="myScrollPane" hbarPolicy="NEVER" layoutX="-1.0" layoutY="37.0" prefHeight="464.0" prefWidth="172.0">
         <content>
            <VBox fx:id="myBox" alignment="TOP_CENTER" prefHeight="464.0" prefWidth="172.0" />
         </content>
      </ScrollPane>
      <Group fx:id="myGroup" layoutX="250.0" layoutY="36.0" />
   </children>
</AnchorPane>
